{
  "name": "azure-git-reporter",
  "version": "1.0.0",
  "main": "main.js",
  "author": {
    "email": "pl.rybson@gmail.com",
    "name": "rybson"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch",
    "lint": "ng lint",
    "lint:style": "npx stylelint '**/*.scss'",
    "prepare": "husky install",
    "prerelease": "npm run build -- -c production",
    "electron": "npm run build -- -c production && electron .",
    "release:mac": "npm run prerelease && electron-builder --mac --publish=never",
    "release:win": "npm run prerelease && electron-builder --win --publish=never",
    "release:linux": "npm run prerelease && electron-builder --linux --publish=never",
    "release:portable": "npm run prerelease && electron-builder --win portable --publish=never"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "date-fns": "^3.3.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.1",
    "primeicons": "^6.0.1",
    "primeng": "^17.3.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.0",
    "@angular-eslint/builder": "17.2.1",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular-eslint/schematics": "17.2.1",
    "@angular-eslint/template-parser": "17.2.1",
    "@angular/cli": "^17.1.0",
    "@angular/compiler-cli": "^17.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jasmine": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "electron": "^28.1.4",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "jasmine-core": "^5.1.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-spec-reporter": "^0.0.36",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "stylelint": "^16.2.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "~5.2.2"
  }
}
